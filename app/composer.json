{
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        },
        "classmap": [""]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "require": {
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "izniburak/router": "^2.0",
        "twig/twig": "^2.0",
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "roave/security-advisories": "dev-master"
        ,
        "phpunit/phpunit": "^8.0"
    },
    "minimum-stability": "dev"
}
